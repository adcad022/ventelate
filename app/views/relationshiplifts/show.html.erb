<div class="page-header">
  <h3>Relationshiplift #<%= @relationshiplift.id %></h3>
</div>

<div class="row">
  <div class="col-md-12">
    <dl class="dl-horizontal">
      <dt>User</dt>
      <dd><%= @relationshiplift.user_id %></dd>

      <dt>Relationshipvent</dt>
      <dd><%= @relationshiplift.relationshipvent_id %></dd>

      <dt>Support</dt>
      <dd><%= @relationshiplift.support %></dd>

    </dl>

    <div class="btn-group btn-group-justified">
      <a href="/relationshiplifts" class="btn btn-primary">
        Back
      </a>
      <a href="/relationshiplifts/<%= @relationshiplift.id %>/edit" class="btn btn-warning">
        Edit
      </a>
      <a href="/delete_relationshiplift/<%= @relationshiplift.id %>" class="btn btn-danger" rel="nofollow">
        Delete
      </a>
    </div>
  </div>
</div>
