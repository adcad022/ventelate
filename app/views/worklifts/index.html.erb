<div class="page-header">
  <h1>
    Worklifts
    <a href="/worklifts/new" class="btn btn-lg btn-success">New Worklift</a>
  </h1>
</div>

<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>
        <th>User</th>
        <th>Workvent</th>
        <th>Support</th>
        <th>Actions</th>
      </tr>

      <% @worklifts.each do |worklift| %>
      <tr>
        <td><%= worklift.user_id %></td>
        <td><%= worklift.workvent_id %></td>
        <td><%= worklift.support %></td>
        <td>
          <a href="/worklifts/<%= worklift.id %>" class="btn btn-primary">Show</a>
          <a href="/worklifts/<%= worklift.id %>/edit" class="btn btn-warning">Edit</a>
          <a href="/delete_worklift/<%= worklift.id %>" class="btn btn-danger" rel="nofollow">Delete</a>
        </td>
      </tr>
      <% end %>
    </table>
  </div>
</div>
