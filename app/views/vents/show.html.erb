<div class="page-header">
  <h3>Vent #<%= @vent.id %></h3>
</div>

<div class="row">
  <div class="col-md-12">
    <dl class="dl-horizontal">
      <dt>Topic</dt>
      <dd><%= @vent.topic %></dd>

      <dt>Urgency</dt>
      <dd><%= @vent.urgency %></dd>

      <dt>Story</dt>
      <dd><%= @vent.story %></dd>

    </dl>

    <div class="btn-group btn-group-justified">
      <a href="/vents" class="btn btn-primary">
        Back
      </a>
      <a href="/vents/<%= @vent.id %>/edit" class="btn btn-warning">
        Edit
      </a>
      <a href="/delete_vent/<%= @vent.id %>" class="btn btn-danger" rel="nofollow">
        Delete
      </a>
    </div>
  </div>
</div>
