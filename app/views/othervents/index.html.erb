<div class="page-header">
  <h1>
    Othervents
    <a href="/othervents/new" class="btn btn-lg btn-success">New Othervent</a>
  </h1>
</div>

<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>
        <th>Other topic</th>
        <th>Urgency</th>
        <th>Story</th>
        <th>Reply</th>
      </tr>

      <% @othervents.each do |othervent| %>
      <tr>
        <td><%= othervent.other_topic %></td>
        <td><%= othervent.urgency %></td>
        <td><%= othervent.story %></td>
        <td>
          <a href="/responses/new" class="glyphicon glyphicon-heart-empty"></a>
        </td>
        <!-- <td>
          <a href="/othervents/<%= othervent.id %>" class="btn btn-primary">Show</a>
          <a href="/othervents/<%= othervent.id %>/edit" class="btn btn-warning">Edit</a>
          <a href="/delete_othervent/<%= othervent.id %>" class="btn btn-danger" rel="nofollow">Delete</a>
        </td> -->
      </tr>
      <% end %>
    </table>
  </div>
</div>
