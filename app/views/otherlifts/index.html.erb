<div class="page-header">
  <h1>
    Otherlifts
    <a href="/otherlifts/new" class="btn btn-lg btn-success">New Otherlift</a>
  </h1>
</div>

<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>
        <th>User</th>
        <th>Othervent</th>
        <th>Support</th>
        <th>Actions</th>
      </tr>

      <% @otherlifts.each do |otherlift| %>
      <tr>
        <td><%= otherlift.user_id %></td>
        <td><%= otherlift.othervent_id %></td>
        <td><%= otherlift.support %></td>
        <td>
          <a href="/otherlifts/<%= otherlift.id %>" class="btn btn-primary">Show</a>
          <a href="/otherlifts/<%= otherlift.id %>/edit" class="btn btn-warning">Edit</a>
          <a href="/delete_otherlift/<%= otherlift.id %>" class="btn btn-danger" rel="nofollow">Delete</a>
        </td>
      </tr>
      <% end %>
    </table>
  </div>
</div>
